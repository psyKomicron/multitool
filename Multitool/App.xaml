<Application x:Class="Multitool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml"
             Exit="Application_Exit"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Themes/ColorsDictionary.xaml"/>
                <ResourceDictionary Source="Themes/CustomScrollBar.xaml"/>
                <ResourceDictionary Source="Themes/CustomScrollViewer.xaml"/>
                <ResourceDictionary Source="Themes/DarkListViewItemTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkListViewTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkMenuItemTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkMenuTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkTabControlTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkWindowTheme.xaml"/>
                <ResourceDictionary Source="Themes/IconButtonTheme.xaml"/>
                <ResourceDictionary Source="Themes/StandardButtonTheme.xaml"/>
                <ResourceDictionary Source="Themes/DarkTextBoxTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
