<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="LightBlackColor">#2e2e2e</Color>
    <Color x:Key="DarkBlackColor">#212121</Color>
    <Color x:Key="DarkDevBlueColor">#037ffc</Color>
    <Color x:Key="DevBlueColor">#FF569DE5</Color>
    <Color x:Key="DevOrangeColor">#CA5100</Color>

    <SolidColorBrush x:Key="LightBlack" Color="{StaticResource LightBlackColor}"/>
    <SolidColorBrush x:Key="DarkBlack" Color="{StaticResource DarkBlackColor}"/>
    <SolidColorBrush x:Key="DevBlue" Color="{StaticResource DevBlueColor}"/>
    <SolidColorBrush x:Key="DarkDevBlue" Color="{StaticResource DarkDevBlueColor}"/>
    <SolidColorBrush x:Key="DevOrange" Color="{StaticResource DevOrangeColor}"/>

</ResourceDictionary>